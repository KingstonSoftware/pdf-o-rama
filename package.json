{
  "name": "pdf-o-rama",
  "private": true,
  "version": "1.4.0",
  "description": "A tool for better PDF form filling",
  "bin": {
    "pdf-o-rama": "dist/pdf-o-rama.js"
  },
  "main": "dist/PDFTool.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rm -rf dist/* && babel src -d dist -s --ignore *.test.js",
    "debug:snap": "babel-node --inspect-brk src/pdf-o-rama.js",
    "test": "jest",
    "test:debug": "babel-node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules/",
      "scratch/"
    ],
    "testURL": "http://localhost"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KingstonSoftware/pdf-o-rama.git"
  },
  "keywords": [
    "pdf",
    "acroform",
    "form"
  ],
  "author": "John Lyon-Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KingstonSoftware/pdf-o-rama/issues"
  },
  "homepage": "https://github.com/KingstonSoftware/pdf-o-rama#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "jest-cli": "^24.1.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "auto-bind2": "^1.0.3",
    "autobind-decorator": "^2.4.0",
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "hummus": "^1.0.95",
    "json5": "^2.1.0",
    "md5": "^2.2.1",
    "minimist": "^1.2.0",
    "qrcode": "^1.3.3",
    "tmp-promise": "^1.0.5"
  }
}
